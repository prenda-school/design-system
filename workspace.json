{
  "version": 2,
  "projects": {
    "example-nextjs": {
      "root": "apps/example-nextjs",
      "sourceRoot": "apps/example-nextjs",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": "production",
          "options": {
            "root": "apps/example-nextjs",
            "outputPath": "dist/apps/example-nextjs"
          },
          "configurations": {
            "development": {},
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "example-nextjs:build",
            "dev": true
          },
          "configurations": {
            "development": {
              "buildTarget": "example-nextjs:build:development",
              "dev": true
            },
            "production": {
              "buildTarget": "example-nextjs:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "example-nextjs:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/example-nextjs"],
          "options": {
            "jestConfig": "apps/example-nextjs/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/example-nextjs/**/*.{ts,tsx,js,jsx}"]
          }
        }
      },
      "tags": []
    },
    "example-nextjs-e2e": {
      "root": "apps/example-nextjs-e2e",
      "sourceRoot": "apps/example-nextjs-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/example-nextjs-e2e/cypress.json",
            "devServerTarget": "example-nextjs:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "example-nextjs:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/example-nextjs-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["example-nextjs"]
    },
    "spark": {
      "root": "libs/spark",
      "sourceRoot": "libs/spark/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/spark",
            "tsConfig": "libs/spark/tsconfig.lib.json",
            "project": "libs/spark/package.json",
            "entryFile": "libs/spark/src/index.ts",
            "external": [
              "react",
              "react-dom",
              "react-is",
              "@material-ui/core",
              "@material-ui/lab"
            ],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/spark/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/spark/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/spark"],
          "options": {
            "jestConfig": "libs/spark/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "staticDir": ["libs/spark/public"],
            "config": {
              "configFolder": "libs/spark/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/spark",
            "staticDir": ["libs/spark/public"],
            "config": {
              "configFolder": "libs/spark/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "spark-e2e": {
      "root": "apps/spark-e2e",
      "sourceRoot": "apps/spark-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/spark-e2e/cypress.json",
            "tsConfig": "apps/spark-e2e/tsconfig.e2e.json",
            "devServerTarget": "spark:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "spark:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/spark-e2e/**/*.{js,ts}"]
          }
        },
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": false
          }
        }
      }
    },
    "spark-icons": {
      "root": "libs/spark-icons",
      "sourceRoot": "libs/spark-icons/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/spark-icons",
            "tsConfig": "libs/spark-icons/tsconfig.lib.json",
            "project": "libs/spark-icons/package.json",
            "entryFile": "libs/spark-icons/src/index.ts",
            "external": [
              "react",
              "react-dom",
              "react-is",
              "@material-ui/core",
              "react/jsx-runtime"
            ],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/spark-icons/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/spark-icons/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/spark-icons"],
          "options": {
            "jestConfig": "libs/spark-icons/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "workspace": {
      "targets": {
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": true
          }
        }
      },
      "root": "."
    }
  }
}
